{
  "name": "custom-cta-posts",
  "version": "1.1.2",
  "description": "Single WordPress plugin to add custom images into posts",
  "scripts": {
    "sass-admin": "node-sass src/scss/admin/main.scss dist/admin/css/custom-cta-admin.css",
    "sass-public": "node-sass src/scss/public/main.scss dist/public/css/custom-cta-public.css",
    "js-admin": "babel src/js/admin/main.js --out-file dist/admin/js/custom-cta-admin.js",
    "js-public": "babel src/js/public/main.js --out-file dist/public/js/custom-cta-public.js",
    "all-js": "npm run js-admin && npm run js-public",
    "all-sass": "npm run sass-admin && npm run sass-public"
  },
  "author": "Wanderson PJ",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "node-sass": "^4.14.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.3"
  }
}
